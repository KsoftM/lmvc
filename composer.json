{
    "name": "ksoftm/lmvc",
    "description": "lightweight framework for ati final year project.",
    "keywords": [
        "lightweight framework", "lmvc"
    ],
    "type": "project",
    "require": {
        "php": "^8.0",
        "ksoftm/mvc": "^0.0.7"
    },
    "license": "MIT",
    "autoload": {
        "files": [
            "src/system/helper.php"
        ],
        "psr-4": {
            "ksoftm\\": "src/"
        }
    },
    "authors": [
        {
            "name": "KsoftM",
            "email": "ksoftmanagement@gmail.com"
        }
    ],
    "scripts": {
        "post-create-project-cmd": [
            "@php server key:generate"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "version": "0.1.0"
}
