    "keywords": [
        "lightweight framework", "lmvc"
    ],
    "type": "project",
    "require": {
        "php": "^8.0",
        "ksoftm/utility": "^0.1.17",
        "ksoftm/mini-data": "^0.1.17",
        "ksoftm/routing-system": "^0.1.17",
        "ksoftm/mvc": "^0.1.17"
    },
    "license": "MIT",
    "autoload": {
        "files": [
